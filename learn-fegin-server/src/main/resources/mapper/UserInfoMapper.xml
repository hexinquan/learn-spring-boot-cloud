<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fegin.server.mapper.UserInfoMapper">
    <select id="getUserInfo" resultType="com.fegin.server.domain.UserInfo">
        select user_id,user_name,age,user_birth from t_user where user_id=#{userId}
    </select>
</mapper>
